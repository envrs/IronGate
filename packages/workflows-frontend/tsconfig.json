{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "composite": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "types": [
      "@irongate/sdk-backend"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.vue"
  ]
}